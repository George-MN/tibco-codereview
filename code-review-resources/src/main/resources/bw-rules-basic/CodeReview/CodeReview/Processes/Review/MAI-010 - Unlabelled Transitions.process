<?xml version="1.0" encoding="UTF-8"?>
<pd:ProcessDefinition xmlns:xsl="http://www.w3.org/1999/XSL/Transform" xmlns:ns="com/tibco/pe/commands" xmlns:pfx="http://www.tibco.com/schemas/CodeReview/CodeReview/SharedRessources/Schemas/Schema.xsd" xmlns:pd="http://xmlns.tibco.com/bw/process/2003" xmlns:ns2="http://www.tibco.com/pe/DeployedVarsType" xmlns:ns42="http://www.tibco.com/pe/WriteToLogActivitySchema" xmlns:ns1="http://www.tibco.com/namespaces/tnt/plugins/file" xmlns:ns4="http://www.tibco.com/pe/EngineTypes" xmlns:ns3="http://fastconnect.fr/tibco/bw/fccodereview/projectcontent" xmlns:ns0="http://www.tibco.com/namespaces/tnt/plugins/file/v2" xmlns:pfx5="http://www.fastconnect.fr/FCTibcoFactory/CodeReview/RuleCard.xsd" xmlns:xsd="http://www.w3.org/2001/XMLSchema" xmlns:pfx4="http://www.fastconnect.fr/FCTibcoFactory/CodeReview/ReviewResult.xsd" xmlns:tib="http://www.tibco.com/bw/xslt/custom-functions" xmlns:pfx3="http://www.fastconnect.fr/FCCodeReview/ReviewProcessInput.xsd" xmlns:pfx2="http://www.fastconnect.fr/FCTibcoFactory/CodeReview/TestResult.xsd">
    <xsd:import namespace="http://www.fastconnect.fr/FCTibcoFactory/CodeReview/ReviewResult.xsd" schemaLocation="/CodeReview/Resources/XSD/ReviewResult.xsd"/>
    <xsd:import namespace="http://www.fastconnect.fr/FCTibcoFactory/CodeReview/RuleCard.xsd" schemaLocation="/CodeReview/Resources/XSD/RuleCard.xsd"/>
    <pd:name>CodeReview/Processes/Review/MAI-010 - Unlabelled Transitions.process</pd:name>
    <pd:startName>Start</pd:startName>
    <pd:startType ref="pfx3:root"/>
    <pd:startX>58</pd:startX>
    <pd:startY>98</pd:startY>
    <pd:returnBindings>
        <xsl:copy-of select="$Format-result/pfx4:review-result"/>
    </pd:returnBindings>
    <pd:endName>End</pd:endName>
    <pd:endType ref="pfx4:review-result"/>
    <pd:endX>954</pd:endX>
    <pd:endY>98</pd:endY>
    <pd:errorSchemas/>
    <pd:processVariables>
        <AuditProcess>
            <xsd:element name="audit-process">
                <xsd:complexType>
                    <xsd:sequence>
                        <xsd:element name="activity-number" type="xsd:string"/>
                    </xsd:sequence>
                </xsd:complexType>
            </xsd:element>
        </AuditProcess>
    </pd:processVariables>
    <pd:targetNamespace>http://xmlns.example.com/1313588926421</pd:targetNamespace>
    <pd:group name="Group">
        <pd:type>com.tibco.pe.core.LoopGroup</pd:type>
        <pd:resourceType>ae.process.group</pd:resourceType>
        <pd:x>231</pd:x>
        <pd:y>117</pd:y>
        <pd:width>397</pd:width>
        <pd:height>268</pd:height>
        <pd:collapsedWidth>57</pd:collapsedWidth>
        <pd:collapsedHeight>70</pd:collapsedHeight>
        <pd:groupVariables/>
        <pd:joinToggle>inherit</pd:joinToggle>
        <config>
            <pd:groupType>inputLoop</pd:groupType>
            <pd:serializable>false</pd:serializable>
            <pd:over>$Get-file-by-type/ns3:project/ns3:file</pd:over>
            <pd:iterationElementSlot>act</pd:iterationElementSlot>
            <pd:indexSlot>pos</pd:indexSlot>
            <pd:outSlot>Cumul</pd:outSlot>
            <pd:activityOutputName>Map Data</pd:activityOutputName>
            <pd:accumulateOutput>true</pd:accumulateOutput>
        </config>
        <pd:inputBindings/>
        <pd:expanded>true</pd:expanded>
        <pd:transition>
            <pd:from>start</pd:from>
            <pd:to>Transform XML</pd:to>
            <pd:lineType>Default</pd:lineType>
            <pd:lineColor>-16777216</pd:lineColor>
            <pd:conditionType>always</pd:conditionType>
        </pd:transition>
        <pd:transition>
            <pd:from>Map Data</pd:from>
            <pd:to>end</pd:to>
            <pd:lineType>Default</pd:lineType>
            <pd:lineColor>-16777216</pd:lineColor>
            <pd:conditionType>always</pd:conditionType>
        </pd:transition>
        <pd:transition>
            <pd:from>Log</pd:from>
            <pd:to>Map Data</pd:to>
            <pd:lineType>Default</pd:lineType>
            <pd:lineColor>-16777216</pd:lineColor>
            <pd:conditionType>always</pd:conditionType>
        </pd:transition>
        <pd:transition>
            <pd:from>Transform XML</pd:from>
            <pd:to>Parse XML</pd:to>
            <pd:lineType>Default</pd:lineType>
            <pd:lineColor>-16777216</pd:lineColor>
            <pd:conditionType>always</pd:conditionType>
        </pd:transition>
        <pd:transition>
            <pd:from>Parse XML</pd:from>
            <pd:to>Map Data</pd:to>
            <pd:lineType>Default</pd:lineType>
            <pd:lineColor>-16777216</pd:lineColor>
            <pd:conditionType>always</pd:conditionType>
        </pd:transition>
        <pd:transition>
            <pd:from>Catch All</pd:from>
            <pd:to>Log</pd:to>
            <pd:lineType>Default</pd:lineType>
            <pd:lineColor>-16777216</pd:lineColor>
            <pd:conditionType>always</pd:conditionType>
        </pd:transition>
        <pd:activity name="Transform XML">
            <pd:type>com.tibco.plugin.xml.XMLTransformActivity</pd:type>
            <pd:resourceType>ae.activities.XMLTransformActivity</pd:resourceType>
            <pd:x>305</pd:x>
            <pd:y>194</pd:y>
            <config>
                <inputStyle>text</inputStyle>
                <engine>TibcoXML</engine>
                <stylesheet>/CodeReview/Resources/XSLT/Unlabelled Transitions.xslt</stylesheet>
            </config>
            <pd:inputBindings>
                <textInput>
                    <xmlString>
                        <xsl:value-of select="$act/ns3:file"/>
                    </xmlString>
                    <parameter>
                        <name>
                            <xsl:value-of select="'maxXPathLength'"/>
                        </name>
                        <value>
                            <xsl:value-of select="$_globalVariables/ns2:GlobalVariables/CodeReview/Criteria/TransitionConditionMinLength"/>
                        </value>
                        <isXMLDocument>
                            <xsl:value-of select="false()"/>
                        </isXMLDocument>
                    </parameter>
                    <parameter>
                        <name>
                            <xsl:value-of select="'minLabelLength'"/>
                        </name>
                        <value>
                            <xsl:value-of select="$_globalVariables/ns2:GlobalVariables/CodeReview/Criteria/TransitionLabelMinLength"/>
                        </value>
                        <isXMLDocument>
                            <xsl:value-of select="false()"/>
                        </isXMLDocument>
                    </parameter>
                </textInput>
            </pd:inputBindings>
        </pd:activity>
        <pd:activity name="Parse XML">
            <pd:type>com.tibco.plugin.xml.XMLParseActivity</pd:type>
            <pd:resourceType>ae.activities.XMLParseActivity</pd:resourceType>
            <pd:x>420</pd:x>
            <pd:y>194</pd:y>
            <config>
                <inputStyle>text</inputStyle>
                <xsdVersion>default</xsdVersion>
                <validateOutput>true</validateOutput>
                <term>
                    <xsd:element name="results">
                        <xsd:complexType>
                            <xsd:sequence>
                                <xsd:element name="result" minOccurs="0" maxOccurs="unbounded">
                                    <xsd:complexType>
                                        <xsd:sequence>
                                            <xsd:element name="from" type="xsd:string"/>
                                            <xsd:element name="to" type="xsd:string"/>
                                            <xsd:element name="condition" type="xsd:string"/>
                                        </xsd:sequence>
                                    </xsd:complexType>
                                </xsd:element>
                            </xsd:sequence>
                        </xsd:complexType>
                    </xsd:element>
                </term>
            </config>
            <pd:inputBindings>
                <xmlString>
                    <xsl:value-of select="$Transform-XML/xmlString"/>
                </xmlString>
            </pd:inputBindings>
        </pd:activity>
        <pd:activity name="Map Data">
            <pd:type>com.tibco.plugin.mapper.MapperActivity</pd:type>
            <pd:resourceType>ae.activities.MapperActivity</pd:resourceType>
            <pd:x>543</pd:x>
            <pd:y>194</pd:y>
            <config>
                <element ref="pfx4:review-result"/>
            </config>
            <pd:inputBindings>
                <pfx4:review-result>
                    <pfx4:errors>
                        <xsl:if test="$_error/ns4:ErrorReport">
                            <pfx4:error>
                                <pfx4:error-name>
                                    <xsl:value-of select="$_error/ns4:ErrorReport/Class"/>
                                </pfx4:error-name>
                                <pfx4:rule>
                                    <xsl:value-of select="'MAI-010'"/>
                                </pfx4:rule>
                                <pfx4:context>
                                    <xsl:value-of select="$act/ns3:file/@fullName"/>
                                </pfx4:context>
                            </pfx4:error>
                        </xsl:if>
                    </pfx4:errors>
                    <pfx4:rule>
                        <xsl:attribute name="disabled">
                            <xsl:value-of select="$Start/pfx3:root/pfx3:disabled"/>
                        </xsl:attribute>
                        <pfx5:rule>
                            <pfx5:category>
                                <xsl:value-of select="'MAI'"/>
                            </pfx5:category>
                            <pfx5:key>
                                <xsl:value-of select="'MAI-010'"/>
                            </pfx5:key>
                            <pfx5:severity>
                                <xsl:value-of select="&quot;MAJOR&quot;"/>
                            </pfx5:severity>
                            <pfx5:weight>
                                <xsl:value-of select="20"/>
                            </pfx5:weight>
                            <pfx5:infos>
                                <pfx5:name>
                                    <xsl:value-of select="&quot;Unlabelled Transitions&quot;"/>
                                </pfx5:name>
                                <pfx5:description>
                                    <xsl:value-of select="&quot;Unlabelled Transitions&quot;"/>
                                </pfx5:description>
                            </pfx5:infos>
                        </pfx5:rule>
                        <xsl:for-each select="$Parse-XML/results/result">
                            <pfx4:result>
                                <pfx4:resource>
                                    <xsl:value-of select="$act/ns3:file/@fullName"/>
                                </pfx4:resource>
                                <pfx4:key>
                                    <xsl:value-of select="&quot;process&quot;"/>
                                </pfx4:key>
                                <pfx4:value>
                                    <xsl:value-of select="$act/ns3:file/@name"/>
                                </pfx4:value>
                                <pfx4:child>
                                    <pfx4:key>
                                        <xsl:value-of select="local-name(from)"/>
                                    </pfx4:key>
                                    <pfx4:value>
                                        <xsl:value-of select="from"/>
                                    </pfx4:value>
                                </pfx4:child>
                                <pfx4:child>
                                    <pfx4:key>
                                        <xsl:value-of select="local-name(to)"/>
                                    </pfx4:key>
                                    <pfx4:value>
                                        <xsl:value-of select="to"/>
                                    </pfx4:value>
                                </pfx4:child>
                                <pfx4:child>
                                    <pfx4:key>
                                        <xsl:value-of select="local-name(condition)"/>
                                    </pfx4:key>
                                    <pfx4:value>
                                        <xsl:value-of select="condition"/>
                                    </pfx4:value>
                                </pfx4:child>
                            </pfx4:result>
                        </xsl:for-each>
                    </pfx4:rule>
                </pfx4:review-result>
            </pd:inputBindings>
        </pd:activity>
        <pd:activity name="Catch All">
            <pd:type>com.tibco.pe.core.CatchActivity</pd:type>
            <pd:resourceType>ae.activities.catch</pd:resourceType>
            <pd:x>318</pd:x>
            <pd:y>322</pd:y>
            <pd:handler>true</pd:handler>
            <config>
                <catchAll>true</catchAll>
            </config>
            <pd:inputBindings/>
        </pd:activity>
        <pd:activity name="Log">
            <pd:type>com.tibco.pe.core.WriteToLogActivity</pd:type>
            <pd:resourceType>ae.activities.log</pd:resourceType>
            <pd:x>442</pd:x>
            <pd:y>322</pd:y>
            <config>
                <role>User</role>
            </config>
            <pd:inputBindings>
                <ns42:ActivityInput>
                    <message>
                        <xsl:value-of select="tib:render-xml($Catch-All/exceptiondata)"/>
                    </message>
                </ns42:ActivityInput>
            </pd:inputBindings>
        </pd:activity>
    </pd:group>
    <pd:activity name="Format result">
        <pd:type>com.tibco.plugin.mapper.MapperActivity</pd:type>
        <pd:resourceType>ae.activities.MapperActivity</pd:resourceType>
        <pd:x>843</pd:x>
        <pd:y>98</pd:y>
        <config>
            <element ref="pfx4:review-result"/>
        </config>
        <pd:inputBindings>
            <pfx4:review-result>
                <pfx4:errors>
                    <xsl:copy-of select="$Cumul/AccumulatedOutput/pfx4:review-result/pfx4:errors/pfx4:error"/>
                </pfx4:errors>
                <pfx4:rule>
                    <xsl:attribute name="disabled">
                        <xsl:value-of select="$Start/pfx3:root/pfx3:disabled"/>
                    </xsl:attribute>
                    <xsl:choose>
                        <xsl:when test="count($Get-file-by-type/ns3:project/ns3:file) > 0">
                            <xsl:attribute name="grade">
                                <xsl:value-of select="round(100 * (count($Get-file-by-type/ns3:project/ns3:file) - count($Cumul/AccumulatedOutput/pfx4:review-result/pfx4:rule/pfx4:result)) div count($Get-file-by-type/ns3:project/ns3:file))"/>
                            </xsl:attribute>
                        </xsl:when>
                        <xsl:otherwise>
                            <xsl:attribute name="grade">
                                <xsl:value-of select="100"/>
                            </xsl:attribute>
                        </xsl:otherwise>
                    </xsl:choose>
                    <xsl:copy-of select="$Rule-Card/cards/pfx5:rule"/>
                    <xsl:copy-of select="$Cumul/AccumulatedOutput/pfx4:review-result/pfx4:rule/pfx4:result"/>
                </pfx4:rule>
            </pfx4:review-result>
        </pd:inputBindings>
    </pd:activity>
    <pd:activity name="Get file by type">
        <pd:type>com.tibco.pe.core.CallProcessActivity</pd:type>
        <pd:resourceType>ae.process.subprocess</pd:resourceType>
        <pd:x>174</pd:x>
        <pd:y>258</pd:y>
        <config>
            <processName>/CodeReview/Processes/Project explorer/Get file by type.process</processName>
        </config>
        <pd:inputBindings>
            <type>
                <xsl:value-of select="'process'"/>
            </type>
        </pd:inputBindings>
    </pd:activity>
    <pd:activity name="Rule Card">
        <pd:type>com.tibco.plugin.mapper.MapperActivity</pd:type>
        <pd:resourceType>ae.activities.MapperActivity</pd:resourceType>
        <pd:x>704</pd:x>
        <pd:y>98</pd:y>
        <config>
            <element>
                <xsd:element name="cards">
                    <xsd:complexType>
                        <xsd:sequence>
                            <xsd:element ref="pfx5:rule" maxOccurs="unbounded"/>
                        </xsd:sequence>
                    </xsd:complexType>
                </xsd:element>
            </element>
        </config>
        <pd:inputBindings>
            <cards>
                <pfx5:rule>
                    <pfx5:category>
                        <xsl:value-of select="&quot;MAI&quot;"/>
                    </pfx5:category>
                    <pfx5:key>
                        <xsl:value-of select="&quot;MAI-010&quot;"/>
                    </pfx5:key>
                    <pfx5:severity>
                        <xsl:value-of select="&quot;MAJOR&quot;"/>
                    </pfx5:severity>
                    <pfx5:weight>
                        <xsl:value-of select="20"/>
                    </pfx5:weight>
                    <pfx5:infos>
                        <pfx5:name>
                            <xsl:value-of select="&quot;Unlabelled Transitions&quot;"/>
                        </pfx5:name>
                        <pfx5:description>
                            <xsl:value-of select="&quot;Unlabelled Transitions&quot;"/>
                        </pfx5:description>
                    </pfx5:infos>
                </pfx5:rule>
            </cards>
        </pd:inputBindings>
    </pd:activity>
    <pd:transition>
        <pd:from>Start</pd:from>
        <pd:to>Get file by type</pd:to>
        <pd:lineType>Default</pd:lineType>
        <pd:lineColor>-16777216</pd:lineColor>
        <pd:conditionType>otherwise</pd:conditionType>
    </pd:transition>
    <pd:transition>
        <pd:from>Format result</pd:from>
        <pd:to>End</pd:to>
        <pd:lineType>Default</pd:lineType>
        <pd:lineColor>-16777216</pd:lineColor>
        <pd:conditionType>always</pd:conditionType>
    </pd:transition>
    <pd:transition>
        <pd:from>Group</pd:from>
        <pd:to>Rule Card</pd:to>
        <pd:lineType>Default</pd:lineType>
        <pd:lineColor>-16777216</pd:lineColor>
        <pd:conditionType>always</pd:conditionType>
    </pd:transition>
    <pd:transition>
        <pd:from>Get file by type</pd:from>
        <pd:to>Group</pd:to>
        <pd:lineType>Default</pd:lineType>
        <pd:lineColor>-16777216</pd:lineColor>
        <pd:conditionType>always</pd:conditionType>
    </pd:transition>
    <pd:transition>
        <pd:from>Start</pd:from>
        <pd:to>Rule Card</pd:to>
        <pd:xpathDescription>Disabled</pd:xpathDescription>
        <pd:lineType>Default</pd:lineType>
        <pd:lineColor>-16777216</pd:lineColor>
        <pd:conditionType>xpath</pd:conditionType>
        <pd:xpath>$Start/pfx3:root/pfx3:disabled = 'true'</pd:xpath>
    </pd:transition>
    <pd:transition>
        <pd:from>Rule Card</pd:from>
        <pd:to>Format result</pd:to>
        <pd:lineType>Default</pd:lineType>
        <pd:lineColor>-16777216</pd:lineColor>
        <pd:conditionType>always</pd:conditionType>
    </pd:transition>
</pd:ProcessDefinition>