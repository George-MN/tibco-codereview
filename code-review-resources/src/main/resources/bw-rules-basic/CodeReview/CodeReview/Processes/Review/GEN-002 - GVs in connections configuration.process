<?xml version="1.0" encoding="UTF-8"?>
<pd:ProcessDefinition xmlns:pd="http://xmlns.tibco.com/bw/process/2003" xmlns:xsl="http://www.w3.org/1999/XSL/Transform" xmlns:ns="http://fastconnect.fr/tibco/bw/fccodereview/projectcontent" xmlns:pfx5="http://www.fastconnect.fr/FCTibcoFactory/CodeReview/RuleCard.xsd" xmlns:xsd="http://www.w3.org/2001/XMLSchema" xmlns:pfx4="http://www.fastconnect.fr/FCTibcoFactory/CodeReview/ReviewResult.xsd" xmlns:tib="http://www.tibco.com/bw/xslt/custom-functions" xmlns:pfx3="http://www.fastconnect.fr/FCTibcoFactory/CodeReview/TestResult.xsd" xmlns:pfx2="http://www.tibco.com/schemas/AuditProcesses/CodeReview/SharedRessources/Configuration/TestGlobalVariablesInConnectionDefinitions/Schema.xsd" xmlns:pfx="http://www.fastconnect.fr/FCCodeReview/ReviewProcessInput.xsd">
    <xsd:import namespace="http://www.fastconnect.fr/FCTibcoFactory/CodeReview/ReviewResult.xsd" schemaLocation="/CodeReview/Resources/XSD/ReviewResult.xsd"/>
    <xsd:import namespace="http://www.fastconnect.fr/FCCodeReview/ReviewProcessInput.xsd" schemaLocation="/CodeReview/Resources/XSD/ReviewProcessInput.xsd"/>
    <xsd:import namespace="http://www.fastconnect.fr/FCTibcoFactory/CodeReview/RuleCard.xsd" schemaLocation="/CodeReview/Resources/XSD/RuleCard.xsd"/>
    <pd:name>CodeReview/Processes/Review/GEN-002 - GVs in connections configuration.process</pd:name>
    <pd:startName>Start</pd:startName>
    <pd:startType ref="pfx:root"/>
    <pd:startX>58</pd:startX>
    <pd:startY>98</pd:startY>
    <pd:returnBindings>
        <pfx4:review-result>
            <xsl:copy-of select="$Format-result/pfx4:review-result/pfx4:rule"/>
        </pfx4:review-result>
    </pd:returnBindings>
    <pd:endName>End</pd:endName>
    <pd:endType ref="pfx4:review-result"/>
    <pd:endX>1018</pd:endX>
    <pd:endY>98</pd:endY>
    <pd:errorSchemas/>
    <pd:processVariables/>
    <pd:targetNamespace>http://xmlns.example.com/1313596880593</pd:targetNamespace>
    <pd:group name="For each connection type">
        <pd:type>com.tibco.pe.core.LoopGroup</pd:type>
        <pd:resourceType>ae.process.group</pd:resourceType>
        <pd:x>292</pd:x>
        <pd:y>165</pd:y>
        <pd:width>185</pd:width>
        <pd:height>169</pd:height>
        <pd:collapsedWidth>57</pd:collapsedWidth>
        <pd:collapsedHeight>70</pd:collapsedHeight>
        <pd:groupVariables/>
        <pd:joinToggle>inherit</pd:joinToggle>
        <config>
            <pd:groupType>inputLoop</pd:groupType>
            <pd:serializable>false</pd:serializable>
            <pd:over>$Get-configuration/pfx2:root/pfx2:connection-type</pd:over>
            <pd:iterationElementSlot>act</pd:iterationElementSlot>
            <pd:indexSlot>pos</pd:indexSlot>
            <pd:outSlot>accu</pd:outSlot>
            <pd:activityOutputName>TestConnectionType</pd:activityOutputName>
            <pd:accumulateOutput>true</pd:accumulateOutput>
        </config>
        <pd:inputBindings/>
        <pd:expanded>true</pd:expanded>
        <pd:transition>
            <pd:from>start</pd:from>
            <pd:to>TestConnectionType</pd:to>
            <pd:lineType>Default</pd:lineType>
            <pd:lineColor>-16777216</pd:lineColor>
            <pd:conditionType>always</pd:conditionType>
        </pd:transition>
        <pd:transition>
            <pd:from>TestConnectionType</pd:from>
            <pd:to>end</pd:to>
            <pd:lineType>Default</pd:lineType>
            <pd:lineColor>-16777216</pd:lineColor>
            <pd:conditionType>always</pd:conditionType>
        </pd:transition>
        <pd:activity name="TestConnectionType">
            <pd:type>com.tibco.pe.core.CallProcessActivity</pd:type>
            <pd:resourceType>ae.process.subprocess</pd:resourceType>
            <pd:x>383</pd:x>
            <pd:y>258</pd:y>
            <config>
                <processName>/CodeReview/Processes/Review/Test global variables in connection definitions/Test connection type.process</processName>
            </config>
            <pd:inputBindings>
                <root>
                    <xsl:copy-of select="$act/pfx2:connection-type"/>
                </root>
            </pd:inputBindings>
        </pd:activity>
    </pd:group>
    <pd:activity name="Format result">
        <pd:type>com.tibco.plugin.mapper.MapperActivity</pd:type>
        <pd:resourceType>ae.activities.MapperActivity</pd:resourceType>
        <pd:x>907</pd:x>
        <pd:y>98</pd:y>
        <config>
            <element ref="pfx4:review-result"/>
        </config>
        <pd:inputBindings>
            <pfx4:review-result>
                <pfx4:rule>
                    <xsl:attribute name="disabled">
                        <xsl:value-of select="$Start/pfx:root/pfx:disabled"/>
                    </xsl:attribute>
                    <xsl:choose>
                        <xsl:when test="(count($Get-file-by-type-JDBC/ns:project/ns:file) + count($Get-file-by-type-JMS/ns:project/ns:file))>0">
                            <xsl:attribute name="grade">
                                <xsl:value-of select="round(100 - (count($accu/AccumulatedOutput/pfx4:global-variable-in-connection-definition-error/pfx4:connection) * 100) div &#xA;(count($Get-file-by-type-JDBC/ns:project/ns:file) + count($Get-file-by-type-JMS/ns:project/ns:file)))"/>
                            </xsl:attribute>
                        </xsl:when>
                        <xsl:otherwise>
                            <xsl:attribute name="grade">
                                <xsl:value-of select="100"/>
                            </xsl:attribute>
                        </xsl:otherwise>
                    </xsl:choose>
                    <xsl:copy-of select="$Rule-Card/cards/pfx5:rule"/>
                    <xsl:for-each select="$accu/AccumulatedOutput/pfx4:global-variable-in-connection-definition-error/pfx4:connection">
                        <pfx4:result>
                            <pfx4:resource>
                                <xsl:value-of select="pfx4:dir"/>
                            </pfx4:resource>
                            <pfx4:key>
                                <xsl:value-of select="local-name(pfx4:dir)"/>
                            </pfx4:key>
                            <pfx4:value>
                                <xsl:value-of select="pfx4:dir"/>
                            </pfx4:value>
                            <xsl:for-each select="pfx4:xpath">
                                <pfx4:child>
                                    <pfx4:key>
                                        <xsl:value-of select="local-name(.)"/>
                                    </pfx4:key>
                                    <pfx4:value>
                                        <xsl:value-of select="."/>
                                    </pfx4:value>
                                </pfx4:child>
                            </xsl:for-each>
                        </pfx4:result>
                    </xsl:for-each>
                </pfx4:rule>
            </pfx4:review-result>
        </pd:inputBindings>
    </pd:activity>
    <pd:activity name="Get configuration">
        <pd:type>com.tibco.pe.core.GetSharedVariableActivity</pd:type>
        <pd:resourceType>ae.activities.getSharedVariable</pd:resourceType>
        <pd:x>181</pd:x>
        <pd:y>258</pd:y>
        <config>
            <variableConfig>/CodeReview/Resources/Shared Variables/Connection parameters.sharedvariable</variableConfig>
        </config>
        <pd:inputBindings/>
    </pd:activity>
    <pd:activity name="Get file by type (JDBC)">
        <pd:type>com.tibco.pe.core.CallProcessActivity</pd:type>
        <pd:resourceType>ae.process.subprocess</pd:resourceType>
        <pd:x>654</pd:x>
        <pd:y>234</pd:y>
        <config>
            <processName>/CodeReview/Processes/Project explorer/Get file by type.process</processName>
        </config>
        <pd:inputBindings>
            <type>
                <xsl:value-of select="'sharedjdbc'"/>
            </type>
        </pd:inputBindings>
    </pd:activity>
    <pd:activity name="Get file by type (JMS)">
        <pd:type>com.tibco.pe.core.CallProcessActivity</pd:type>
        <pd:resourceType>ae.process.subprocess</pd:resourceType>
        <pd:x>747</pd:x>
        <pd:y>330</pd:y>
        <config>
            <processName>/CodeReview/Processes/Project explorer/Get file by type.process</processName>
        </config>
        <pd:inputBindings>
            <type>
                <xsl:value-of select="'sharedjmscon'"/>
            </type>
        </pd:inputBindings>
    </pd:activity>
    <pd:activity name="Rule Card">
        <pd:type>com.tibco.plugin.mapper.MapperActivity</pd:type>
        <pd:resourceType>ae.activities.MapperActivity</pd:resourceType>
        <pd:x>768</pd:x>
        <pd:y>98</pd:y>
        <config>
            <element>
                <xsd:element name="cards">
                    <xsd:complexType>
                        <xsd:sequence>
                            <xsd:element ref="pfx5:rule" maxOccurs="unbounded"/>
                        </xsd:sequence>
                    </xsd:complexType>
                </xsd:element>
            </element>
        </config>
        <pd:inputBindings>
            <cards>
                <pfx5:rule>
                    <pfx5:category>
                        <xsl:value-of select="&quot;GEN&quot;"/>
                    </pfx5:category>
                    <pfx5:key>
                        <xsl:value-of select="&quot;GEN-002&quot;"/>
                    </pfx5:key>
                    <pfx5:severity>
                        <xsl:value-of select="&quot;MAJOR&quot;"/>
                    </pfx5:severity>
                    <pfx5:weight>
                        <xsl:value-of select="10"/>
                    </pfx5:weight>
                    <pfx5:infos>
                        <pfx5:name>
                            <xsl:value-of select="&quot;Connection configuration hardcoded&quot;"/>
                        </pfx5:name>
                        <pfx5:description>
                            <xsl:value-of select="&quot;All fields of a connection should be parameterized with Global Variables.&quot;"/>
                        </pfx5:description>
                        <pfx5:descriptionHTML>
                            <xsl:value-of select="&quot;All fields of a connection should be parameterized with Global Variables.&quot;"/>
                        </pfx5:descriptionHTML>
                    </pfx5:infos>
                </pfx5:rule>
            </cards>
        </pd:inputBindings>
    </pd:activity>
    <pd:transition>
        <pd:from>Start</pd:from>
        <pd:to>Get configuration</pd:to>
        <pd:lineType>Default</pd:lineType>
        <pd:lineColor>-16777216</pd:lineColor>
        <pd:conditionType>otherwise</pd:conditionType>
    </pd:transition>
    <pd:transition>
        <pd:from>Get configuration</pd:from>
        <pd:to>For each connection type</pd:to>
        <pd:lineType>Default</pd:lineType>
        <pd:lineColor>-16777216</pd:lineColor>
        <pd:conditionType>always</pd:conditionType>
    </pd:transition>
    <pd:transition>
        <pd:from>For each connection type</pd:from>
        <pd:to>Rule Card</pd:to>
        <pd:lineType>Default</pd:lineType>
        <pd:lineColor>-16777216</pd:lineColor>
        <pd:conditionType>always</pd:conditionType>
    </pd:transition>
    <pd:transition>
        <pd:from>Format result</pd:from>
        <pd:to>End</pd:to>
        <pd:lineType>Default</pd:lineType>
        <pd:lineColor>-16777216</pd:lineColor>
        <pd:conditionType>always</pd:conditionType>
    </pd:transition>
    <pd:transition>
        <pd:from>Start</pd:from>
        <pd:to>Rule Card</pd:to>
        <pd:xpathDescription>Disabled</pd:xpathDescription>
        <pd:lineType>Default</pd:lineType>
        <pd:lineColor>-16777216</pd:lineColor>
        <pd:conditionType>xpath</pd:conditionType>
        <pd:xpath>$Start/pfx:root/pfx:disabled = 'true'</pd:xpath>
    </pd:transition>
    <pd:transition>
        <pd:from>For each connection type</pd:from>
        <pd:to>Get file by type (JDBC)</pd:to>
        <pd:lineType>Default</pd:lineType>
        <pd:lineColor>-16777216</pd:lineColor>
        <pd:conditionType>always</pd:conditionType>
    </pd:transition>
    <pd:transition>
        <pd:from>For each connection type</pd:from>
        <pd:to>Get file by type (JMS)</pd:to>
        <pd:lineType>Default</pd:lineType>
        <pd:lineColor>-16777216</pd:lineColor>
        <pd:conditionType>always</pd:conditionType>
    </pd:transition>
    <pd:transition>
        <pd:from>Get file by type (JDBC)</pd:from>
        <pd:to>Rule Card</pd:to>
        <pd:lineType>Default</pd:lineType>
        <pd:lineColor>-16777216</pd:lineColor>
        <pd:conditionType>always</pd:conditionType>
    </pd:transition>
    <pd:transition>
        <pd:from>Get file by type (JMS)</pd:from>
        <pd:to>Rule Card</pd:to>
        <pd:lineType>Default</pd:lineType>
        <pd:lineColor>-16777216</pd:lineColor>
        <pd:conditionType>always</pd:conditionType>
    </pd:transition>
    <pd:transition>
        <pd:from>Rule Card</pd:from>
        <pd:to>Format result</pd:to>
        <pd:lineType>Default</pd:lineType>
        <pd:lineColor>-16777216</pd:lineColor>
        <pd:conditionType>always</pd:conditionType>
    </pd:transition>
</pd:ProcessDefinition>