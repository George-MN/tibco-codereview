                                    <xsd:sequence>
                                        <xsd:element name="property" type="xsd:string" minOccurs="0" maxOccurs="unbounded"/>
                                    </xsd:sequence>
                                    <xsd:attribute name="prefix" type="xsd:string" use="required"/>
                                </xsd:complexType>
                            </xsd:element>
                        </xsd:sequence>
                    </xsd:complexType>
                </xsd:element>
            </element>
        </config>
        <pd:inputBindings>
            <root>
                <vendor>
                    <!--TIBCO EMS-->
                    <xsl:attribute name="prefix">
                        <xsl:value-of select="'JMS_TIBCO_'"/>
                    </xsl:attribute>
