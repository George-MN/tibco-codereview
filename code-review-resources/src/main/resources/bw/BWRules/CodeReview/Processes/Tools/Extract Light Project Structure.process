<?xml version="1.0" encoding="UTF-8"?>
<pd:ProcessDefinition xmlns:ns2="http://www.tibco.com/pe/DeployedVarsType" xmlns:pd="http://xmlns.tibco.com/bw/process/2003" xmlns:ns1="http://www.tibco.com/namespaces/tnt/plugins/file" xmlns:xsl="http://www.w3.org/1999/XSL/Transform" xmlns:ns="com/tibco/pe/commands" xmlns:ns0="http://www.tibco.com/namespaces/tnt/plugins/file/v2" xmlns:xsd="http://www.w3.org/2001/XMLSchema" xmlns:pfx4="http://fastconnect.fr/tibco/bw/fccodereview/projectcontent" xmlns:tib="http://www.tibco.com/bw/xslt/custom-functions" xmlns:pfx3="http://fastconnect.fr/tibco/bw/fccodereview/lightprocess" xmlns:pfx2="http://www.fastconnect.fr/FCTibcoFactory/CodeReview/TestResult.xsd" xmlns:pfx="http://www.tibco.com/schemas/BWRules/CodeReview/SharedRessources/Schemas/Schema.xsd">
    <xsd:import namespace="http://www.fastconnect.fr/FCTibcoFactory/CodeReview/TestResult.xsd" schemaLocation="/CodeReview/Resources/XSD/ReviewResult.xsd"/>
    <xsd:import namespace="http://fastconnect.fr/tibco/bw/fccodereview/lightprocess" schemaLocation="/CodeReview/Resources/XSD/LightProcess.xsd"/>
    <pd:name>CodeReview/Processes/Tools/Extract Light Project Structure.process</pd:name>
    <pd:startName>Start</pd:startName>
    <pd:startX>58</pd:startX>
    <pd:startY>130</pd:startY>
    <pd:returnBindings>
        <xsl:copy-of select="$Build-Light-Project/pfx3:project"/>
    </pd:returnBindings>
    <pd:endName>End</pd:endName>
    <pd:endType ref="pfx3:project"/>
    <pd:endX>730</pd:endX>
    <pd:endY>130</pd:endY>
    <pd:errorSchemas/>
    <pd:processVariables>
        <AuditProcess>
            <xsd:element name="audit-process">
                <xsd:complexType>
                    <xsd:sequence>
                        <xsd:element name="activity-number" type="xsd:string"/>
                    </xsd:sequence>
                </xsd:complexType>
            </xsd:element>
        </AuditProcess>
    </pd:processVariables>
    <pd:targetNamespace>http://xmlns.example.com/1313588926421</pd:targetNamespace>
    <pd:group name="Group">
        <pd:type>com.tibco.pe.core.LoopGroup</pd:type>
        <pd:resourceType>ae.process.group</pd:resourceType>
        <pd:x>249</pd:x>
        <pd:y>36</pd:y>
        <pd:width>286</pd:width>
        <pd:height>171</pd:height>
        <pd:collapsedWidth>57</pd:collapsedWidth>
        <pd:collapsedHeight>70</pd:collapsedHeight>
        <pd:groupVariables/>
        <pd:joinToggle>inherit</pd:joinToggle>
        <config>
            <pd:groupType>inputLoop</pd:groupType>
            <pd:serializable>false</pd:serializable>
            <pd:over>$GetProcesses/pfx4:project/pfx4:file</pd:over>
            <pd:iterationElementSlot>act</pd:iterationElementSlot>
            <pd:indexSlot>pos</pd:indexSlot>
            <pd:outSlot>Accum</pd:outSlot>
            <pd:activityOutputName>Parse XML</pd:activityOutputName>
            <pd:accumulateOutput>true</pd:accumulateOutput>
        </config>
        <pd:inputBindings/>
        <pd:expanded>true</pd:expanded>
        <pd:transition>
            <pd:from>start</pd:from>
            <pd:to>Transform XML</pd:to>
            <pd:lineType>Default</pd:lineType>
            <pd:lineColor>-16777216</pd:lineColor>
            <pd:conditionType>always</pd:conditionType>
        </pd:transition>
        <pd:transition>
            <pd:from>Parse XML</pd:from>
            <pd:to>end</pd:to>
            <pd:lineType>Default</pd:lineType>
            <pd:lineColor>-16777216</pd:lineColor>
            <pd:conditionType>always</pd:conditionType>
        </pd:transition>
        <pd:transition>
            <pd:from>Transform XML</pd:from>
            <pd:to>Parse XML</pd:to>
            <pd:lineType>Default</pd:lineType>
            <pd:lineColor>-16777216</pd:lineColor>
            <pd:conditionType>always</pd:conditionType>
        </pd:transition>
        <pd:activity name="Transform XML">
            <pd:type>com.tibco.plugin.xml.XMLTransformActivity</pd:type>
            <pd:resourceType>ae.activities.XMLTransformActivity</pd:resourceType>
            <pd:x>337</pd:x>
            <pd:y>130</pd:y>
            <config>
                <inputStyle>text</inputStyle>
                <engine>TibcoXML</engine>
                <stylesheet>/CodeReview/Resources/XSLT/LighterProcess.xslt</stylesheet>
            </config>
            <pd:inputBindings>
                <textInput>
                    <xmlString>
                        <xsl:value-of select="$act/pfx4:file"/>
                    </xmlString>
                    <parameter>
                        <name>
                            <xsl:value-of select="'processName'"/>
                        </name>
                        <value>
                            <xsl:value-of select="$act/pfx4:file/@name"/>
                        </value>
                        <isXMLDocument>
                            <xsl:value-of select="false()"/>
                        </isXMLDocument>
                    </parameter>
                </textInput>
            </pd:inputBindings>
        </pd:activity>
        <pd:activity name="Parse XML">
            <pd:type>com.tibco.plugin.xml.XMLParseActivity</pd:type>
            <pd:resourceType>ae.activities.XMLParseActivity</pd:resourceType>
            <pd:x>452</pd:x>
            <pd:y>130</pd:y>
            <config>
                <inputStyle>text</inputStyle>
                <xsdVersion>default</xsdVersion>
                <validateOutput>true</validateOutput>
                <term ref="pfx3:process"/>
            </config>
            <pd:inputBindings>
                <xmlString>
                    <xsl:value-of select="$Transform-XML/xmlString"/>
                </xmlString>
            </pd:inputBindings>
        </pd:activity>
    </pd:group>
    <pd:activity name="GetProcesses">
        <pd:type>com.tibco.pe.core.CallProcessActivity</pd:type>
        <pd:resourceType>ae.process.subprocess</pd:resourceType>
        <pd:x>173</pd:x>
        <pd:y>130</pd:y>
        <config>
            <processName>/CodeReview/Processes/Project explorer/Get file by type.process</processName>
        </config>
        <pd:inputBindings>
            <type>
                <xsl:value-of select="'process'"/>
            </type>
        </pd:inputBindings>
    </pd:activity>
    <pd:activity name="Build Light Project">
        <pd:type>com.tibco.plugin.mapper.MapperActivity</pd:type>
        <pd:resourceType>ae.activities.MapperActivity</pd:resourceType>
        <pd:x>610</pd:x>
        <pd:y>138</pd:y>
        <config>
            <element ref="pfx3:project"/>
        </config>
        <pd:inputBindings>
            <pfx3:project>
                <xsl:copy-of select="$Accum/AccumulatedOutput/pfx3:process"/>
            </pfx3:project>
        </pd:inputBindings>
    </pd:activity>
    <pd:transition>
        <pd:from>Group</pd:from>
        <pd:to>Build Light Project</pd:to>
        <pd:lineType>Default</pd:lineType>
        <pd:lineColor>-16777216</pd:lineColor>
        <pd:conditionType>always</pd:conditionType>
    </pd:transition>
    <pd:transition>
        <pd:from>Start</pd:from>
        <pd:to>GetProcesses</pd:to>
        <pd:lineType>Default</pd:lineType>
        <pd:lineColor>-16777216</pd:lineColor>
        <pd:conditionType>always</pd:conditionType>
    </pd:transition>
    <pd:transition>
        <pd:from>Build Light Project</pd:from>
        <pd:to>End</pd:to>
        <pd:lineType>Default</pd:lineType>
        <pd:lineColor>-16777216</pd:lineColor>
        <pd:conditionType>always</pd:conditionType>
    </pd:transition>
    <pd:transition>
        <pd:from>GetProcesses</pd:from>
        <pd:to>Group</pd:to>
        <pd:lineType>Default</pd:lineType>
        <pd:lineColor>-16777216</pd:lineColor>
        <pd:conditionType>always</pd:conditionType>
    </pd:transition>
</pd:ProcessDefinition>