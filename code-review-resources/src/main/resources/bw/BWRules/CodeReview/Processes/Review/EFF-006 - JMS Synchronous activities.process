                    <xsl:value-of select="concat(&quot;//pd:*[&quot;,&#xA;&#x9;&quot;pd:type='com.tibco.plugin.jms.JMSQueueRequestReplyActivity' or &quot;,&#xA;&#x9;&quot;pd:type='com.tibco.plugin.jms.JMSTopicRequestReplyActivity' &quot;,&#xA;&quot;]/@name&quot;)"/>
                </xpath>
                <ruleName>
                    <xsl:value-of select="BW:getCurrentProcessName($_processContext/ns3:ProcessContext/ProcessId)"/>
                </ruleName>
            </root>
        </pd:inputBindings>
    </pd:activity>
    <pd:transition>
        <pd:from>Start</pd:from>
        <pd:to>Find Activity Calls</pd:to>
        <pd:lineType>Default</pd:lineType>
        <pd:lineColor>-16777216</pd:lineColor>
